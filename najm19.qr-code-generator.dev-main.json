{
    "manifests": {
        "najm19/qr-code-generator": {
            "manifest": {
                "bundles": {
                    "QrGeneratorBundle\\QrGeneratorBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%",
                    "src/": "%SRC_DIR%/"
                },
                "post-install-output": [
                    "  * Your APP is almost ready: Thanks for using najm19/qr-code-generator"
                ],
                "files": {
                    "config/routes/qr_generator.yaml": "config/routes/qr_generator.yaml",
                    "config/routes/qr_generator_bundle.yaml": {
                        "contents": [
                            "qr_generator:",
                            "    path: /qr/generate",
                            "    controller: QrGeneratorBundle\\Controller\\QrGeneratorController::generate",
                            "    methods: [GET]"
                        ],
                        "executable": false
                    }
                },
                "aliases": [
                    "qr",
                    "codes"
                ],
                "env": {
                    "QR_SECRET_KEY": "%kernel.project_dir%/%CONFIG_DIR%/jwt/private.pem",
                    "QR_PUBLIC_KEY": "%kernel.project_dir%/%CONFIG_DIR%/jwt/public.pem",
                    "QR_PASSPHRASE": "%generate(secret, 32)%"
                },
                "gitignore": [
                    "/%CONFIG_DIR%/jwt/*.pem"
                ]
            },
            "ref": "7405f3af1312d1f9121afed4dddef636c6c7ff00"
        }
    }
}
